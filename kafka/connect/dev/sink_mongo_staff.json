{
  "name": "mongo_sink_staff",
  "config": {
    "tasks.max": "1",
    "connector.class": "com.mongodb.kafka.connect.MongoSinkConnector",
    "connection.uri": "mongodb://mongo-user-id:secret-password@iccdomainapis.cluster-cdm9nmobuch0.ap-southeast-2.docdb.amazonaws.com:27017/?ssl=true&replicaSet=rs0&readPreference=secondaryPreferred&retryWrites=false",
    "topics": "staff-hub-streaming",
    "database": "staff",
    "collection": "kafka_staff",
    "document.id.strategy": "com.mongodb.kafka.connect.sink.processor.id.strategy.PartialValueStrategy",
    "delete.on.null.values": "false",
    "max.batch.size": "0",
    "max.num.retries": "3",
    "retries.defer.timeout": "5000",
    "change.data.capture.handler": "",
    "field.renamer.mapping": "",
    "field.renamer.regexp": "",
    "key.projection.list": "",
    "key.projection.type": "none",
    "value.projection.list": "staffIdentifier",
    "value.projection.type": "whitelist",
    "post.processor.chain": "com.mongodb.kafka.connect.sink.processor.DocumentIdAdder",
    "rate.limiting.timeout": "0",
    "rate.limiting.every.n": "0",
    "writemodel.strategy": "com.mongodb.kafka.connect.sink.writemodel.strategy.ReplaceOneBusinessKeyStrategy",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "key.converter.schemas.enable": "false",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "http://kafka-dev-vip.nsw.education:80",
    "transforms": "ReplaceField, InsertField, TimestampConverter",
    "transforms.ReplaceField.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
    "transforms.ReplaceField.blacklist": "metacreatedby, metacreateddate, metaprovidedby",
    "transforms.InsertField.type": "org.apache.kafka.connect.transforms.InsertField$Value",
    "transforms.InsertField.timestamp.field": "kafkaModifiedDate",
    "transforms.TimestampConverter.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.TimestampConverter.field": "kafkaModifiedDate",
    "transforms.TimestampConverter.target.type": "unix"
  }
}
